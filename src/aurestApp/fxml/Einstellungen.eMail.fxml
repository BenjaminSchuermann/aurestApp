<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40" VBox.vgrow="ALWAYS">
    <children>

        <Label id="info" text="Mitarbeiter :"/>

        <ListView id="mitarbeiter" fx:id="mitarbeiter" prefHeight="400.0" prefWidth="300.0" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets top="5.0"/>
            </VBox.margin>
        </ListView>
        <VBox alignment="BOTTOM_RIGHT">
            <children>

                <Button id="speichern" fx:id="speichern" mnemonicParsing="false" onAction="#speicherEinstellungen" text="Speichern"/>
            </children>
            <VBox.margin>
                <Insets top="15.0"/>
            </VBox.margin>
        </VBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
    </padding>
</VBox>
